#include <stdio.h>
#include <string.h>

struct Class {
    int class;
    char name[30];
    int Roll_no;
};

int main() {
    struct Class s1, s2;

    // Input for s1
    printf("Enter class for student 1: ");
    scanf("%d", &s1.class);
    printf("Enter name for student 1: ");
    scanf("%s", s1.name); // Using %s for string input
    printf("Enter Roll_no for student 1: ");
    scanf("%d", &s1.Roll_no);

    // Input for s2
    printf("Enter class for student 2: ");
    scanf("%d", &s2.class);
    printf("Enter name for student 2: ");
    scanf("%s", s2.name); // Using %s for string input
    printf("Enter Roll_no for student 2: ");
    scanf("%d", &s2.Roll_no);

    // Printing the details of both students
    printf("Details of student 1:\n");
    printf("Class: %d\n", s1.class);
    printf("Name: %s\n", s1.name);
    printf("Roll_no: %d\n", s1.Roll_no);

    printf("\nDetails of student 2:\n");
    printf("Class: %d\n", s2.class);
    printf("Name: %s\n", s2.name);
    printf("Roll_no: %d\n", s2.Roll_no);

    return 0;
}
